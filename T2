# T is the centralizer of v2 in 1+Gamma(K4).

gap> T:=[];
[  ]
gap> for a01 in S do
>      	for a02 in S do
>         for a03 in S do
>           for a11 in S do
>             for a12 in S do
>              for a13 in S do
>               for a21 in S do
>                 for a22 in S do
>                   for a23 in S do
>                     x:=i+a01*(a-i)+a02*i*b*(a-i)+a03*i*b^2*(a-i)+a11*(b*a*b^2-i)+a12*i*b*(b*a*b^2-i)+a13*i*b^2*(b*a*b^2-i)+a21*(b^2*a*b-i)+a22*i*b*(b^2*a*b-i)+a23*i*b^2*(b^2*a*b-i);
>                     if x*(i*b+Z(2^2)*(i+i*b^2))=(i*b+Z(2^2)*(i+i*b^2))*x then
>  		               	Append(T,[x]);
>  		                fi;
>                   od;
>                 od;
>               od;
>             od;
>            od;
>          od;
>        od;
>      od;
>    od;
gap>Size(T);
1024

# C is the set of conjugacy classes of all elements of set T, when v is varying over 144 elements of V(FA4). This step is done to get some intuition of the class length of elements of T. Also,
position set contains positions of the elements whose class length is 12 and position1 set contains positions of the elements of class length 1.

gap> position:=[];
[  ]
gap> position1:=[];
[  ]
gap> C:=[];
[  ]
gap> for j in [1..Size(T)] do
> 
>    Append(C,[[]]);
> 
> od;

# V denotes V(FC3). 

gap> V:=[i, i+(Z(2^2))*i*b^-1+(Z(2^2))*i*b, i+(Z(2^2)^2)*i*b^-1+(Z(2^2)^2)*i*b,  b+(Z(2^2))*i*b^-1+(Z(2^2))*i,  b^2+(Z(2^2))*i*b+(Z(2^2))*i, b+(Z(2^2)^2)*i*b^2+(Z(2^2)^2)*i, b^2+(Z(2^2)^2)*i*b+(Z(2^2)^2)*i, b, b^2 ];
[ (Z(2)^0)*<identity ...>, (Z(2)^0)*<identity ...>+(Z(2^2))*b^-1+(Z(2^2))*b, (Z(2)^0)*<identity ...>+(Z(2^2)^2)*b^-1+(Z(2^2)^2)*b, 
  (Z(2^2))*<identity ...>+(Z(2^2))*b^-1+(Z(2)^0)*b, (Z(2^2))*<identity ...>+(Z(2)^0)*b^-1+(Z(2^2))*b, (Z(2^2)^2)*<identity ...>+(Z(2^2)^2)*b^-1+(Z(2)^0)*b,
  (Z(2^2)^2)*<identity ...>+(Z(2)^0)*b^-1+(Z(2^2)^2)*b, b, b^-1 ]
gap> for t in T do
> 	k:=Position(T,t);
> 	empty:=0;
> 		for a13 in S do
> 		   for a23 in S do
>  		      for w in V do
>  		         v:=(i+a13*i*b^2*(i*b*a*b^2-i)+a23*i*b^2*(i*b^2*a*b-i))*(i*w);
> 		         x:=v^-1*t*v;
> 		         if x in C[k] then
> 		         else
> 		            Append(C[k],[x]);
> 		         fi;
> 		      od;
> 		    od;
> 		od;
> 		for p in [1..k-1] do;
> 			if Intersection(C[p],C[k])=[] then
> 			else empty:=1;
> 			fi;
> 		od;
> 		if empty=0 then
> 			if Size(C[k])=1 then
> 			   Append(position1,[k]);
> 			else
> 			   Append(position,[k]);
> 			fi;
> 		fi;
> od;
gap> Size(position);
80
gap> position;
